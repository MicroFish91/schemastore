{
  "revisionSuffix": "suffix",
  "terminationGracePeriodSeconds": 40,
  "initContainers": null,
  "containers": [
      {
          "image": "registryname.azurecr.io/imagename:tag",
          "name": "imagename-tag",
          "env": [
              {
                  "name": "TEST_KEY",
                  "value": "abcd1234"
              }
          ],
          "resources": {
              "cpu": 0.5,
              "memory": "1Gi",
              "ephemeralStorage": "2Gi"
          }
      }
  ],
  "scale": {
      "minReplicas": 1,
      "maxReplicas": 5,
      "rules": [
          {
              "name": "httprule",
              "http": {
                  "metadata": {
                      "concurrentRequests": "25"
                  }
              }
          }
      ]
  },
  "volumes": null,
  "serviceBinds": null
}
